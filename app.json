{
  "expo": {
    "name": "Tefereth Scripts",
    "slug": "teferenth-expo",
    "version": "1.0.0",
    "sdkVersion": "53.0.0",
    "platforms": ["ios", "android", "web"],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f9fafb"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shegosthato.teferenthexpo"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#f9fafb"
      },
      "package": "com.shegosthato.teferenthexpo",
      "softwareKeyboardLayoutMode": "pan",
      "permissions": [],
      "userInterfaceStyle": "light",
      "backgroundColor": "#f9fafb"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "output": "static",
      "name": "Tefereth Scripts",
      "shortName": "Tefereth",
      "lang": "en-US",
      "themeColor": "#f9fafb",
      "description": "A script writing application",
      "orientation": "portrait",
      "backgroundColor": "#f9fafb",
      "barStyle": "default",
      "display": "standalone",
      "startUrl": "/",
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#f9fafb"
      },
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "c1940d5c-efc5-424d-b146-7cfe706156c4"
      },
      "AI_API_BASE_URL": "https://api.a0.dev",
      "AI_LLM_ENDPOINT": "/ai/llm",
      "IMAGE_GENERATION_ENDPOINT": "/assets/image",
      "MAX_FILE_SIZE_MB": "1",
      "MAX_CONTENT_LENGTH": "50000",
      "AI_TIMEOUT": "30000",
      "IMAGE_TIMEOUT": "45000",
      "ENABLE_OFFLINE_MODE": "false",
      "ENABLE_ANALYTICS": "false",
      "ENABLE_CRASH_REPORTING": "false",
      "DEBUG_MODE": "false",
      "LOG_LEVEL": "info",
      "MOCK_API_RESPONSES": "false"
    },
    "owner": "shegosthato"
  }
}
